{
    "name": "@isarmstrong/jitterbug-next",
    "version": "0.1.6",
    "description": "Next.js integration for Jitterbug logger",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "prepack": "npm run build",
        "test": "vitest",
        "lint": "eslint src --ext .ts"
    },
    "peerDependencies": {
        "@isarmstrong/jitterbug": "^0.1.6",
        "next": ">=13.0.0",
        "react": ">=18.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/react": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "next": "^15.1.6",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./api": {
            "types": "./dist/api/index.d.ts",
            "import": "./dist/api/index.js",
            "require": "./dist/api/index.js",
            "default": "./dist/api/index.js"
        },
        "./transports/sse": {
            "types": "./dist/transports/sse/index.d.ts",
            "import": "./dist/transports/sse/index.js",
            "require": "./dist/transports/sse/index.js",
            "default": "./dist/transports/sse/index.js"
        }
    }
}