{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "dist/core",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "composite": true,
    "declaration": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@transports/*": [
        "src/transports/*"
      ],
      "@ebl/*": [
        "src/types/ebl/*"
      ],
      "@isarmstrong/jitterbug": [
        "src"
      ],
      "@isarmstrong/jitterbug/*": [
        "src/*"
      ],
      "@isarmstrong/jitterbug-types": [
        "packages/jitterbug-types/src"
      ],
      "@isarmstrong/jitterbug-types/*": [
        "packages/jitterbug-types/src/*"
      ]
    }
  },
  "references": [
    {
      "path": "./src"
    },
    {
      "path": "./packages/jitterbug-types"
    },
    {
      "path": "./packages/jitterbug-next"
    }
  ],
  "files": [],
  "include": []
}